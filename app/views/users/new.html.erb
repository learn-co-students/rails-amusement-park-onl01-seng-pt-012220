<h1>Signup</h1>

<%= form_for @user do |f| %>
  <%= f.label :name, "Test Name" %>
  <%= f.text_field :name %>
  </br>

  <%= f.label :height, "Height in Inches" %>
  <%= f.text_field :height %>
  </br>

  <%= f.label :happiness%>
  <%= f.text_field :happiness %>
  </br>

  <%= f.label :nausea%>
  <%= f.text_field :nausea %>
  </br>

  <%= f.label :tickets%>
  <%= f.text_field :tickets %>
  </br>

  <%= f.label :admin%>
  <%= f.check_box :admin %>
  </br>


  <%= f.label :password %>
  <%= f.password_field :password %>
  </br>

  <%= f.submit %>
<% end %>